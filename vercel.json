{
    "buildCommand": "prisma generate && next build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "functions": {
        "src/app/api/**/*.ts": {
            "maxDuration": 30
        }
    },
    "env": {
        "PRISMA_GENERATE_DATAPROXY": "true",
        "PRISMA_GENERATE_SKIP_DOWNLOAD": "true"
    }
}
